<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/iv_call_js"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Android Call JS By JavascriptInterface"/>

    <Button
        android:id="@+id/iv_evaluate"
        android:layout_below="@+id/iv_call_js"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Android Call JS By Evaluate"/>

    <WebView
        android:id="@+id/web_nomi"
        android:layout_below="@+id/iv_evaluate"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    </WebView>
</RelativeLayout>
